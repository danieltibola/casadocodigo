<html>
    <body>
        <table border="1px">
            <thead>
                <th>id</th>
                <th>titulo</th>
                <th>descricao</th>
                <th>preco</th>
            </thead>
            <tbody>
                <%for(var i=0; i<lista.length;i++) {%>
                <tr>
                    <td><%=lista[i].id%></td>
                    <td><%=lista[i].titulo%></td>
                    <td><%=lista[i].descricao%></td>
                    <td><%=lista[i].preco%></td>
                </tr> 
                <%}%>
            </tbody>
        </table>
    </body>
</html>
